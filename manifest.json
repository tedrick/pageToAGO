{
  "name": "Publish Page to ArcGIS Online",
  "version": "0.1",
  "description": "Save a URL as an item in your ArcGIS Online account",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "plus.png",
    "default_title": "Save to ArcGIS Online"
  },
  "permissions": [
    "activeTab", "tabs", "identity",
      "https://www.arcgis.com/"
  ],
  "manifest_version": 2
}
